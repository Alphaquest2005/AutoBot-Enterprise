<Application x:Class="WaterNut.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary 
                Source="Styles/DataGridCustomizations.xaml"/>

                <ResourceDictionary 
                Source="Styles/Theme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="CancelImage" >Cancel.png</BitmapImage>
        </ResourceDictionary>
    </Application.Resources>
</Application>

